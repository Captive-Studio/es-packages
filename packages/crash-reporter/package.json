{
  "name": "@captive/crash-reporter",
  "version": "1.0.0-local.0",
  "private": true,
  "description": "A hub for crash reports. Many backends (Sentry, Raygun, ...), to many frontends (VueJS, ...)",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*"
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "!**/*.spec.*",
    "!**/__tests__/**"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" ",
    "build:tsc": "tsc -b tsconfig.build.json",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "clean:tsc": "rm -rf dist",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "test": "vitest run",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@types/raygun4js": "2.13.10"
  },
  "devDependencies": {
    "@capacitor/core": "5.5.1",
    "@sentry/capacitor": "^0.14.0",
    "raygun4js": "2.28.0",
    "vite": "4.5.0",
    "vitest": "0.34.6",
    "vue": "3.3.7"
  },
  "peerDependencies": {
    "@sentry/capacitor": "0.14.0",
    "raygun4js": "^2.25.6",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "@sentry/capacitor": {
      "optional": true
    },
    "raygun4js": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
